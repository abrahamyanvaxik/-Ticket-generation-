<h1>Add Ticket</h1>

 {{#if data}}
  <div class="row">
     <div class="col s12">Unique name of the file: {{data.fileName}}</div>
     <div class="col s12">
          <a href="{{data.url}}">Download</a>
     </div>
   </div>
 {{/if}}

  <div class="row">
    <form class="col s12" action="/" method="POST">
      <div class="row">
        <div class="input-field col s12">
          <input placeholder="Name" id="name" name="name" type="text" class="validate" required>
          <label for="disabled">Ticket Name</label>
          <span class="helper-text" data-error="Please ticket name."></span>
        </div>
      </div>
       <div class="row">
        <div class="input-field col s12">
          <input placeholder="Code" id="price" name="code" type="text" class="validate" required>
          <label for="disabled">Ticket code</label>
          <span class="helper-text" data-error="Please ticket code."></span>
        </div>
      </div>
       <div class="row">
            <div class="input-field col s12">
                <select material-select name="code_type" id="code_type">
                  <option value="qrcode" selected>Qr code</option>
                  <option value="barcode">Barcodes</option>
                </select>
                <label>Choose code type.</label>
                <span class="helper-text" data-error="Please select code type."></span>
            </div>
      </div>
      <button class="btn btn-primary">Save</button>
    </form>
  </div>